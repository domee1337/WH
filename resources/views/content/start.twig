<link rel="stylesheet" href="{{ plugin_path('Warehouse') }}/css/basic.css">
<!-- -->
<div id='load' style='display: none;'>
  <div class="spinner">
    <div class="rect1"></div>
    <div class="rect2"></div>
    <div class="rect3"></div>
    <div class="rect4"></div>
    <div class="rect5"></div>
  </div>
</div>

<div class="container">
  <h1>Start</h1>
  <!-- Warehouses[0] START -->
  <div class="warehouses">
    {% if warehouses is not null %}
      {% for warehouse in warehouses %}
        <label>
          <input type="checkbox" class="warehousecheckbox" whid="{{ warehouse.id }}" > {{ warehouse.name }}

        &nbsp;</label>
      {% endfor %}
      <input type="text" class="form-control findArticle" placeholder="EAN">
      <div id="output"></div>
      <input type="number" id="menge" value="1" class="form-control"><input type="checkbox" id="standart_menge"><p>Standardisierte Menge</p>
      <input type="text" id="lagerortean" class="form-control">
    {% else %}
      <p>Bitte legen Sie ein Lager an</p>
    {% endif %}
  </div>
  <!-- Warehouses[0] END -->
</div>
